    "from math import lgamma\n",
    "import numpy as np\n",
    "import math\n",
    "\n",
    "def my_f(n):\n",
    "    return n/np.e**(lgamma(n + 1) / n)\n",
    "\n",
    "acc = 0.0000001\n",
    "i=2\n",
    "while(abs(my_f(i) - my_f(i-1))) > acc:\n",
    "    i +=1\n",
    "\n",
    "print(my_f(i), abs(my_f(i) - my_f(i-1)), i)"
   ]
